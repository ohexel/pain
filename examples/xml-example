				<fund_ack>
					<grants count="1">
						<grant grant_source="Medline">
							<grant_agency>NIDA NIH HHS</grant_agency>
							<grant_ids count="4">
								<grant_id>R01 DA007242</grant_id>
								<grant_id>R01 DA002615</grant_id>
								<grant_id>K05 DA000220</grant_id>
								<grant_id>R01 DA006241</grant_id>
